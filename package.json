{
  "name": "bolji-pogled",
  "displayName": "bolji_pogled",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bolji-pogled.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "bolji-pogled.activateNoLines",
        "title": "Activate No Lines"
      }
    ],
    "themes": [
      {
        "label": "normal-dark-theme",
        "uiTheme": "vs-dark",
        "path": "./themes/normal-dark-theme.json"
      }
    ],
    "themes": [
      {
        "label": "normal-dark-theme",
        "uiTheme": "vs-dark",
        "path": "./themes/normal-dark-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
